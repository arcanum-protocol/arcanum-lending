{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/arev\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/manager\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/use\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}